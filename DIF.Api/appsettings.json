{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "DIF.Api": "Debug"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5000"
      }
    }
  },
  "DIF": {
    "Distributors": {
      "SS": {
        "Name": "S&S Activewear",
        "RateLimitPerMinute": 60,
        "ThresholdPercentage": 90,
        "Enabled": true
      },
      "IMG": {
        "Name": "IMG",
        "RateLimitPerMinute": 100,
        "ThresholdPercentage": 90,
        "Enabled": true
      },
      "SanMar": {
        "Name": "SanMar",
        "RateLimitPerMinute": 120,
        "ThresholdPercentage": 90,
        "Enabled": true
      }
    },
    "Alerts": {
      "SlackChannel": "#dif-alerts",
      "EmailRecipients": [
        "ops-team@freshprints.com",
        "engineering@freshprints.com"
      ],
      "CriticalAlertDelaySeconds": 60,
      "WarningBatchIntervalHours": 2
    },
    "Tracking": {
      "PollIntervalHours": 12,
      "MaxRetries": 3,
      "RetryDelaysSeconds": [1, 5, 15]
    }
  }
}
